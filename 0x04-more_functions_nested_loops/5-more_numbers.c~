#include "main.h"
/*
* more_numbers - prints 10 times the numbers,
* from 0-14, followed by a new line
* return - none
*/void more_numbers(void)
{
int i;
int n;
for (i = 1; i <= 10; i++)
{
for (n = 1; n <= 14; n++)
{
if (n >= 10)
{
_putchar((n / 10) + '0')
}
_putchar((n % 10) + '0')
}
_putchar('\n');
}
return;
}
